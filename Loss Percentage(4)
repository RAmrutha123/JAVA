
import java.util.Scanner;
class Loss
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        double x = sc.nextDouble();
        double y = sc.nextDouble();
        double l = x-y;
        double p = (l/x)*100;
        System.out.printf("%.2f" ,p);
    }
}
