
import java.util.*;
class Dig{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int count=0;
        while(n!=0){
            int d=n%10;
            count++;
            n/=10;

        }
        System.out.println(count);
        sc.close();
    }
}
