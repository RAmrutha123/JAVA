
import java.util.*;
class Capacity{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        int s=sc.nextInt();
        int b=sc.nextInt();
        int total=2*t*s*b*512;
        int cap=total/1024;
        System.out.println(cap+" "+"KB");
        sc.close();
    }
}
