
import java.util.*;
class Even{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=sc.nextInt();}
        boolean b=true;
        for(int num : a){
            if(num%2!=0){
                b=false;
                break;}
        }
        if(b){System.out.println("True");}
        else{System.out.println("False");}
        sc.close();
    }
}
